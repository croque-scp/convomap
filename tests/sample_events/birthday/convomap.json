  "strings": [
    {
      "id": 0,
      "string": "Hey, Mark.",
      "messageId": 0
    },
    {
      "id": 1,
      "string": "Hi, Janet. What's up?",
      "messageId": 1
    },
    {
      "id": 2,
      "string": "I hear it's your birthday party on Saturday.",
      "messageId": 2
    },
    {
      "id": 3,
      "string": "Might I ask why I wasn't invited?",
      "messageId": 3
    },
    {
      "id": 4,
      "string": "I'm sorry - I forgot.",
      "optionId": 0
    },
    {
      "id": 5,
      "string": "I just don't like you.",
      "optionId": 1
    },
    {
      "id": 6,
      "string": "There's not going to be a birthday party.",
      "optionId": 2
    },
    {
      "id": 7,
      "string": "I'm so sorry! I must have forgotten to send yours.",
      "messageId": 4
    },
    {
      "id": 8,
      "string": "That's alright! Don't worry about it.",
      "messageId": 5
    },
    {
      "id": 9,
      "string": "It happens to everyone.",
      "messageId": 6
    },
    {
      "id": 10,
      "string": "I'll see you on Saturday?",
      "messageId": 7
    },
    {
      "id": 11,
      "string": "Sounds good! I'll see you there.",
      "messageId": 8
    },
    {
      "id": 12,
      "string": "Janet, in truth, I simply do not like you.",
      "messageId": 9
    },
    {
      "id": 13,
      "string": "It really just comes down to that.",
      "messageId": 10
    },
    {
      "id": 14,
      "string": "I see.",
      "messageId": 11
    },
    {
      "id": 15,
      "string": "Well, thank you for being honest with me.",
      "messageId": 12
    },
    {
      "id": 16,
      "string": "I'm glad I now know where we stand.",
      "messageId": 13
    },
    {
      "id": 17,
      "string": "I'm sorry, Janet - I didn't mean it like that.",
      "optionId": 3
    },
    {
      "id": 18,
      "string": "Good.",
      "optionId": 4
    },
    {
      "id": 19,
      "string": "I'm so sorry - you must have heard incorrectly.",
      "messageId": 14
    },
    {
      "id": 20,
      "string": "I'm not having a birthday party on Saturday.",
      "messageId": 15
    },
    {
      "id": 21,
      "string": "Who told you that?",
      "messageId": 16
    },
    {
      "id": 22,
      "string": "Are you sure? I trust my sources.",
      "messageId": 17
    },
    {
      "id": 23,
      "string": "It's not even my birthday for months.",
      "optionId": 5
    },
    {
      "id": 24,
      "string": "It was cancelled.",
      "optionId": 6
    },
    {
      "id": 25,
      "string": "I was lying.",
      "optionId": 7
    },
    {
      "id": 26,
      "string": "Well that's not true.",
      "messageId": 18
    },
    {
      "id": 27,
      "string": "You invited me last year.",
      "messageId": 19
    },
    {
      "id": 28,
      "string": "You must be misremembering.",
      "messageId": 20
    }
  ]
